<%= search_form_for @q, url: posts_path do |f| %>
  <%= f.label :category_name_cont, "業界:" %>
  <%= f.collection_select(:category_name_cont, Category.all, :name, :name, prompt: "-- 選択してください --" ) %>
  <%= f.label :sub_category_name_cont, "職種:" %>
  <%= f.collection_select(:sub_category_name_cont, SubCategory.all, :name, :name, prompt: "-- 選択してください --" ) %>
  <%= f.submit "検索", class: 'search-q-btn' %>
<% end %>

